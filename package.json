{
  "name": "canto",
  "version": "1.0.0",
  "description": "Canto monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "modules/*"
  ],
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:web\" \"bun run dev:dashboard\"",
    "dev:web": "cd apps/web && bun run dev",
    "dev:server": "cd apps/server && bun run dev",
    "dev:dashboard": "cd apps/dashboard && bun run dev"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^16.4.0",
    "knip": "^5.72.0",
    "prettier": "3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  }
}
